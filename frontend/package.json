    "@types/source-map": "0.5.2",
    "angular-auth-oidc-client": "12.0.3",
    "angular-plotly.js": "^4.0.4",
    "base64-js": "^1.5.1",
    "guid-typescript": "^1.0.9",
    "hd-wiring": "0.5.1",
    "hetida-flowchart": "0.5.1",
    "js-sha256": "^0.9.0",
    "katex": "^0.11.1",
    "marked": "^0.7.0",
    "material-design-icons-iconfont": "^5.0.1",
    "ng-hetida-flowchart": "0.5.0",
    "ngx-monaco-editor": "^9.0.0",
    "plotly.js": "^1.58.2",
