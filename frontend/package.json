{
  "name": "hetida-designer-frontend",
  "description": "Hetida Designer Frontend",
  "version": "0.1.0",
  "homepage": "https://neusta-sd-west.de/data-science/hetida-designer/",
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0 <13.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hetida/hetida-designer"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "compile-libs": "ngcc",
    "build": "ng build",
    "test": "ng test",
    "test-docker": "ng test --karma-config ./src/karma-docker.conf.js",
    "lint": "ng lint",
    "format:all": "prettier --write 'src/**/*.{js,ts,json,html,scss}'",
    "format:staged": "pretty-quick --staged",
    "e2e-all": "playwright test --config=./playwright/playwright.config.ts",
    "e2e-chromium": "playwright test --config=./playwright/playwright.config.ts --project=chromium",
    "mock:server": "json-server --watch data.json",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "test-headless": "ng test --watch=false --browsers=ChromeHeadless"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run format:staged"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.2.15",
    "@angular/cdk": "12.2.13",
    "@angular/common": "^12.2.15",
    "@angular/compiler": "^12.2.15",
    "@angular/core": "^12.2.15",
    "@angular/forms": "^12.2.15",
    "@angular/material": "12.2.13",
    "@angular/platform-browser": "^12.2.15",
    "@angular/platform-browser-dynamic": "^12.2.15",
    "@angular/router": "^12.2.15",
    "@danielmoncada/angular-datetime-picker": "^12.3.0",
    "@ngrx/entity": "^12.5.1",
    "@ngrx/store": "^12.5.1",
    "@types/source-map": "0.5.2",
    "angular-auth-oidc-client": "12.0.3",
    "angular-plotly.js": "^4.0.4",
    "base64-js": "^1.5.1",
    "guid-typescript": "^1.0.9",
    "hd-wiring": "0.3.10",
    "hetida-flowchart": "0.3.0",
    "js-sha256": "^0.9.0",
    "katex": "^0.11.1",
    "marked": "^0.7.0",
    "material-design-icons-iconfont": "^5.0.1",
    "ng-hetida-flowchart": "0.3.0",
    "ngx-monaco-editor": "^9.0.0",
    "plotly.js": "^1.58.2",
    "rxjs": "^6.6.7",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.15",
    "@angular/cli": "^12.2.15",
    "@angular/compiler-cli": "^12.2.15",
    "@angular/language-service": "^12.2.15",
    "@ngrx/store-devtools": "^12.5.1",
    "@playwright/test": "^1.18.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/katex": "^0.10.2",
    "@types/marked": "^0.7.4",
    "@types/node": "~14.0.27",
    "@types/plotly.js": "^1.54.4",
    "@types/uuid": "^3.4.9",
    "codelyzer": "^6.0.0",
    "husky": "^4.3.5",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "json-server": "^0.17.0",
    "karma": "^6.3.17",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "moment": "^2.29.2",
    "ng-packagr": "^12.2.6",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "ts-node": "~8.10.2",
    "tslint": "~6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "~4.3.5"
  }
}
