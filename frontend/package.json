{
  "name": "hetida-designer-frontend",
  "description": "Hetida Designer Frontend",
  "version": "0.1.0",
  "homepage": "https://neusta-sd-west.de/data-science/hetida-designer/",
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0 <13.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hetida/hetida-designer"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "compile-libs": "ngcc",
    "build": "ng build",
    "test": "ng test",
    "test-docker": "ng test --karma-config ./src/karma-docker.conf.js",
    "lint": "ng lint",
    "format:all": "prettier --write \"src/**/*.{js,ts,json,html,scss}\"",
    "format:staged": "pretty-quick --staged",
    "e2e-all": "playwright test --config=./playwright/playwright.config.ts",
    "e2e-chromium": "playwright test --config=./playwright/playwright.config.ts --project=chromium",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "test-headless": "ng test --watch=false --browsers=ChromeHeadless"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run format:staged"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.4.0",
    "@angular/cdk": "13.3.9",
    "@angular/common": "^13.4.0",
